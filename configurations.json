    "definitions": {
        "vtec_regexp": "[OTEX].(NEW|CON|EXT|EXA|EXB|UPG|CAN|EXP|COR|ROU).[A-Z]{4}.[A-Z]{2}.[WAYSFON].[0-9]{4}.[0-9]{6}T[0-9]{4}Z-[0-9]{6}T[0-9]{4}Z",
        "ugc_start_regexp": "(\\w{2}[CZ](\\d{3}((-|>)\\s?(\n\n)?))+)",
        "wmo_regexp": "[A-Z0-9]{6}\\s[A-Z]{4}\\s\\d{6}",
        "ugc_end_regexp": "(\\d{6}(-|>)\\s?(\n\n)?)",
        "cancel_signatures": [ "has been cancelled", "will be allowed to expire", "has diminished", "and no longer", "has been replaced", "The threat has ended", "has weakened below severe" ],
        "status_signatures": { "NEW": "Issued", "CON": "Updated", "EXT": "Extended", "EXA": "Extended", "EXB": "Extended", "UPG": "Upgraded", "COR": "Correction", "ROU": "Routine", "CAN": "Cancelled", "EXP": "Expired" },
